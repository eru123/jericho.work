{
    "minimum-stability": "dev",
    "require": {
        "eru123/holyphp": "dev-main",
        "aws/aws-sdk-php": "^3.275"
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload --optimize"
        ],
        "post-update-cmd": [
            "composer dump-autoload --optimize"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        },
        "files": [
            "App/functions.php"
        ]
    }
}